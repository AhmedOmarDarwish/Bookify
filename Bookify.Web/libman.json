{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootbox.js@6.0.0",
      "destination": "wwwroot/lib/bootbox.js/"
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate.css/",
      "files": [
        "animate.min.css",
        "animate.css"
      ]
    },
    {
      "library": "sweetalert2@11.16.1",
      "destination": "wwwroot/lib/sweetalert2/sweetalert2/"
    },
    {
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "flatpickr@4.6.13",
      "destination": "wwwroot/lib/flatpickr/",
      "files": [
        "flatpickr.min.js",
        "flatpickr.min.css"
      ]
    },
    {
      "library": "bootstrap-daterangepicker@3.0.5",
      "destination": "wwwroot/lib/bootstrap-daterangepicker/",
      "files": [
        "daterangepicker.min.js",
        "moment.min.js",
        "daterangepicker.min.css"
      ]
    },
    {
      "library": "select2@4.0.13",
      "destination": "wwwroot/lib/select2/",
      "files": [
        "js/select2.min.js",
        "css/select2.min.css"
      ]
    }
